{
  "load_extensions": {
    "comment-uncomment/main": true,
    "nbextensions_configurator/config_menu/main": true,
    "contrib_nbextensions_help_item/main": true,
    "jupyter-js-widgets/extension": true
  },
  "keys": {
      "command": {
      "bind": {
        "ctrl-\u00ba": "auto:toggle-comment",
        "ctrl-\u00e7": "auto:toggle-comment",
        "ctrl-\u00ab": "auto:toggle-comment"
      }
    }
  },
  "Cell": {
    "cm_config": {
      "lineNumbers": false
    }
  },
  "Notebook": {
    "Header": true,
    "Toolbar": true
  }
}